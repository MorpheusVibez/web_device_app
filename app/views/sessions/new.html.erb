<h1> This is the login page </h1>
<%= binding.pry %>
<%=form_for @user do |f| %>
  <%=f.label :username %>
  <%=f.text_field :username %> 
  
  <%= f.label :password %>
  <%= f.password_field :password %>

  <%= f.submit "log in" %>
  Or <%= link_to(controller: 'users', action: 'new') { 'sign up' } %>
<% end %>


